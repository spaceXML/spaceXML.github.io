&lt;p&gt;select *
from EMP
WHere DEPTNO = 30;&lt;/p&gt;

&lt;p&gt;select EMPNO, ENAME, JOB
from EMP
where JOB = ‘MANAGER’;
–1) EMP 테이블에서 전체 사원의 empno, ename, sal,
– deptno 값을 출력&lt;/p&gt;

&lt;p&gt;select empno, ename, sal, deptno
from EMP
